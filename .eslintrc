{
  "extends": "airbnb",
  "plugins": ["async-await"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "rules": {
    "strict": 0,
    "class-methods-use-this": 0,
    "no-multi-str": 0,
    "object-curly-spacing": 0,
    "padded-blocks": 0,
    "spaced-comment": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    "comma-dangle": 0,
    "pluginName": 0
  }
}